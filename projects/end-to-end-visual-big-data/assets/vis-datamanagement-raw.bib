	title = {Voyager 2: {Augmenting} {Visual} {Analysis} with {Partial} {View} {Specifications}},
	isbn = {978-1-4503-4655-9},
	shorttitle = {Voyager 2},
	url = {https://doi.org/10.1145/3025453.3025768},
