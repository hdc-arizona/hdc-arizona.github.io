}

@article{liu_immens:_2013,
	title = {{imMens}: {Real}-time {Visual} {Querying} of {Big} {Data}},
	volume = {32},
	copyright = {© 2013 The Author(s) Computer Graphics Forum © 2013 The Eurographics Association and Blackwell Publishing Ltd.},
